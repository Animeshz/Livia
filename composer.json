{
    "name": "charlottedunois/livia",
    "description": "Livia is a Discord Bot framework, which utilizes Yasmin, the Discord API library.",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "CharlotteDunois",
            "email": "github@charuru.moe"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "charlottedunois/yasmin": "^0.6.0|dev-master",
        "charlottedunois/validator": "^0.2.0",
        "symfony/var-dumper": "^4.1"
    },
    "require-dev": {
        "plasma/core": "^0.1.0|dev-master",
        "react/mysql": "^0.4.1"
